//Coding Ninjas


//Rabbit and Tortoise Method
public class Solution
{
    public static Node findMiddle(Node head)
    {
        Node ptr1=head;
        Node ptr2=head;
        while(ptr2!=null && ptr2.next!=null){
            ptr1=ptr1.next;
            
            ptr2=ptr2.next.next;
            
        }
        return ptr1;
    }
}
